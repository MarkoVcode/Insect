<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="keywords" content="">
    <title>Insect - Peek</title>

    <!-- Styles -->
    <link href="/assets/css/theDocs.all.min.css" rel="stylesheet">
    <link href="/assets/css/custom.css" rel="stylesheet">
    <link href="/assets/insect/css/insect.css" rel="stylesheet">
    <!-- Fonts -->
    <link href='//fonts.googleapis.com/css?family=Raleway:100,300,400,500%7CLato:300,400' rel='stylesheet' type='text/css'>

  </head>

  <body id="screengrab-main-content">

    <header class="site-header">

      <!-- Top navbar & branding -->
      <nav class="navbar navbar-default">
        <div class="container-fluid">

          <!-- Toggle buttons and brand -->
          <div class="navbar-header">
            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#navbar" aria-expanded="true" aria-controls="navbar">
              <span class="glyphicon glyphicon-option-vertical"></span>
            </button>

              <a class="navbar-brand" href="/"><img src="/assets/img/logo.png" alt="logo"></a>
          </div>
          <!-- END Toggle buttons and brand -->

          <!-- Top navbar -->
          <div id="navbar" class="navbar-collapse collapse" aria-expanded="true" role="banner">
            <ul class="nav navbar-nav navbar-right">
                <li>
                <button type="button" class="btn btn-lg btn-default">{{psid}}</button>
                <button type="button" class="btn btn-lg btn-danger" id="indicator-ws">Offline</button>
{{#has_ownership}}
                {{^is_active}}
                    <button type="button" class="btn btn-lg btn-danger" id="indicator-proxy">Proxy Inactive</button>
                {{/is_active}}
                {{#is_active}}
                    <button type="button" class="btn btn-lg btn-success" id="indicator-proxy">Proxy Active</button>
                {{/is_active}}
                <button type="button" class="btn btn-lg btn-danger" id="session-timeout">00:00</button>
{{/has_ownership}}
                </li>
            </ul>
          </div>
          <!-- END Top navbar -->

        </div>
      </nav>
      <!-- END Top navbar & branding -->

    </header>

    <main class="container-fluid">

      <div class="row">

          <article class="col-md-12 main-content" role="main">
              <button type="button" id="proxy-reset" class="btn btn-xs btn-primary">{{^has_ownership}}Get yourself own session!{{/has_ownership}}
                  {{#has_ownership}}Get yourself a new session!{{/has_ownership}}</button>

          <div class="tabs tabs-text">

              <!-- Nav tabs -->
              <ul class="nav nav-tabs" role="tablist">
                  <li role="presentation" class="active">
                      <a href="#proxy" aria-controls="home" role="tab" data-toggle="tab">Proxy</a>
                  </li>
                  <li role="presentation">
                      <a href="#mock1" aria-controls="profile" role="tab" data-toggle="tab">Mock-1</a>
                  </li>
                  <li role="presentation">
                      <a href="#mock2" aria-controls="messages" role="tab" data-toggle="tab">Mock-2</a>
                  </li>
                  <li role="presentation">
                      <a href="#mock3" aria-controls="settings" role="tab" data-toggle="tab">Mock-3</a>
                  </li>
                  <li role="presentation">
                      <a href="#mock4" aria-controls="settings" role="tab" data-toggle="tab">Mock-4</a>
                  </li>
                  <li role="presentation">
                      <a href="#mock5" aria-controls="settings" role="tab" data-toggle="tab">Mock-5</a>
                  </li>
              </ul>

              <!-- Tab panes -->
              <div class="tab-content">
                  <div role="tabpanel" class="tab-pane active fade in" id="proxy">
                      <table class="table table-bordered table-striped table-info">
                          <tbody>
                          <tr>
                              <td>Endpoint:</td>
                              <td><strong id="endpoint-url">{{new_api_endpoint}}</strong>&nbsp;&nbsp;<button type="button" id="proxy-endpoint-clipboard" class="btn btn-xs btn-purple" data-clipboard-action="copy" data-clipboard-target="#endpoint-url">Copy to clipboard</button></td>
                              <td>{{#has_ownership}}
                                  {{^is_active}}
                                      <button type="button" id="proxy-endpoint-testget" class="btn btn-xs btn-purple" disabled>Make test GET request</button>
                                  {{/is_active}}
                                  {{#is_active}}
                                      <button type="button" id="proxy-endpoint-testget" class="btn btn-xs btn-purple">Make test GET request</button>
                                  {{/is_active}}
                              {{/has_ownership}}</td>
                          </tr>
                          <tr>
                              <td>Upstream:</td>
                              <td>
                                  {{^has_ownership}}
                                      <input type="text" id="api-endpoint" value="{{proxy_api_endpoint}}" size="70" disabled>
                                  {{/has_ownership}}
                                  {{#has_ownership}}
                                      {{^is_active}}
                                          <input type="text" id="api-endpoint" value="{{proxy_api_endpoint}}" size="80">
                                      {{/is_active}}
                                      {{#is_active}}
                                          <input type="text" id="api-endpoint" value="{{proxy_api_endpoint}}" size="80" disabled>
                                      {{/is_active}}
                                  {{/has_ownership}}
                                  <div id="api-endpoint-error"></div>
                              </td>
                              <td>{{#has_ownership}}
                                  {{^is_active}}
                                      <button type="button" id="proxy-upstream-selftest" class="btn btn-xs btn-purple">Use demo upstream</button>
                                  {{/is_active}}
                                  {{#is_active}}
                                      <button type="button" id="proxy-upstream-selftest" class="btn btn-xs btn-purple" disabled>Use demo upstream</button>
                                  {{/is_active}}
                              {{/has_ownership}}</td>
                          </tr>
                          {{#has_ownership}}
                              <tr>
                                  <td></td>
                                  <td>
                                      {{^is_active}}
                                          <button type="button" id="proxy-start" class="btn btn-lg btn-success"><i class="fa fa-caret-square-o-right"></i> Start</button>
                                          <button type="button" id="proxy-stop" class="btn btn-lg btn-danger" disabled><i class="fa fa-square"></i> Stop</button>
                                      {{/is_active}}
                                      {{#is_active}}
                                          <button type="button" id="proxy-start" class="btn btn-lg btn-success" disabled><i class="fa fa-caret-square-o-right"></i> Start</button>
                                          <button type="button" id="proxy-stop" class="btn btn-lg btn-danger"><i class="fa fa-square"></i> Stop</button>
                                      {{/is_active}}
                                  </td>
                                  <td></td>
                              </tr>
                          {{/has_ownership}}
                          </tbody>
                      </table>
                  </div>
                  <div role="tabpanel" class="tab-pane fade" id="mock1">mock1</div>
                  <div role="tabpanel" class="tab-pane fade" id="mock2">mock2</div>
                  <div role="tabpanel" class="tab-pane fade" id="mock3">mock3</div>
                  <div role="tabpanel" class="tab-pane fade" id="mock4">mock4</div>
                  <div role="tabpanel" class="tab-pane fade" id="mock5">mock5</div>
              </div>

          </div>
        </article>

          <article class="col-md-12 main-content">
              <div id="result-buttons" style="display: none;">
                  <button type="button" id="proxy-print-result" class="btn btn-xs btn-purple">Capture output!</button>&nbsp;<button type="button" id="proxy-result-clearall" class="btn btn-xs btn-purple">Clear all</button>
              </div>
              <div id="proxy-updates"></div>
          </article>
        <!-- END Main content  -->
      </div>
    </main>


    <!-- Footer -->
    <footer class="site-footer">
      <div class="container-fluid">
        <a id="scroll-up" href="#"><i class="fa fa-angle-up"></i></a>

        <div class="row">
          <div class="col-md-6 col-sm-6">
              <p><img src="/assets/insect/softcognito_f03.png" height="24" width="117">&nbsp;&nbsp;Copyright &copy; 2016. All right reserved</p>
          </div>
          <div class="col-md-6 col-sm-6">
            <ul class="footer-menu">
                <li><a class="active" href="/changelog">Changelog</a></li>
            </ul>
          </div>
        </div>
      </div>
    </footer>
    <!-- END Footer -->

    <!-- Scripts -->
    <script>
    var configObj = {
        websocketsURL: '{{config_websockets_url}}',
        selftestURL: '{{selftest_url}}',
        sessionTimeout: {{config_session_timeout}},
        psid: '{{psid}}'
        };
    </script>
    <script src="/assets/js/theDocs.all.min.js"></script>
    <script src="/assets/js/custom.js"></script>
    <script src="/assets/insect/js/html2canvas.js"></script>
    <script src="/assets/insect/js/clipboard.min.js"></script>
    <script src="/assets/insect/js/peek.js"></script>
  </body>
</html>
